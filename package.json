    "babel-plugin-transform-rename-import": "^2.1.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "diff": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-nodemailer": "^1.2.0",
    "eslint-plugin-import": "^2.11.0",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.1.1"
  },
  "engines": {
    "node": ">=4.0.0",
    "yarn": "^1.6.0"
  },
  "directories": {
    "lib": "lib",
    "doc": "docs"
